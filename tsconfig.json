{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2020",
		"lib": ["es2020"],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"rootDir": "src",
		"allowSyntheticDefaultImports": true
	},
	"exclude": ["node_modules", ".vscode-test", "relay", "out"]
}
